// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
void heapify(int arr[],int n,int i){
    int c1=2*i+1,c2=2*i+2;
    int min=i;
    if(c1<n&&arr[c1]>arr[min]){
        min=c1;
    }
    if(c2<n&&arr[c2]>arr[min]){
        min=c2;
    }
    if(min!=i){
        int temp=0;
        temp=arr[min];
        arr[min]=arr[i];
        arr[i]=temp;
        heapify(arr,n,min);
    }
}
void heap(int arr[],int n){ int i=0;
    for( i=n/2-1;i>=0;i--){
        heapify(arr,n,i);
    }
        for(i=n-1;i>=0;i--){
            int t=0;
            t=arr[0];
            arr[0]=arr[i];
            arr[i]=t;
            heapify(arr,i,0);
        }
    }

int main() {
  int loop;
  cout<<"enter number of loops"<<endl;
  cin>>loop;
  while(loop!=0){
      int n;
      cout<<"enter number of elements"<<endl;
      cin>>n;
      int arr[n],i=0;
      cout<<"enter elemets in an array"<<endl;
      for(i=0;i<n;i++){
          cin>>arr[i];
      }
      heap(arr,n);
      int key=0,flag=0, p=0,h=0;
      cout<<"enter the number"<<endl;
      cin>>key;
      for(i=0;i<n-1;i++){
          int j=i+1;
          if(arr[i]+arr[j]==key)
          { flag=1;
           p=i;
           h=j;
          break;
           }
           else 
           j++;
           flag++;
      }
      
      if(n==flag)
      cout<<"no such elemets exists"<<endl;
      else if(flag==1){
          cout<<arr[p]<<"  "<<arr[h];
      }
           
     loop--;
  }
    return 0;
}
