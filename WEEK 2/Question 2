 #include<iostream>
 using namespace std;
 int main(){
 int loop;
  cout<<"enter number of loops"<<endl;
  cin>>loop;
  while(loop!=0){
  int i,j,k,n,c=0;
  cout<<"enter number of elements"<<endl;
  cin>>n;
  int arr[n];
  cout<<"enter elements in an array"<<endl;
  for(i=0;i<n;i++)
  {
      cin>>arr[i];
  }
  for(k=0;k<n;k++){
  for(i=0;i<=n-1;i++){
  for(j=i+1;j<n;j++){
      if((arr[i]+arr[j])==arr[k]){
     c++;
     cout<<(i+1)<<", "<<(j+1)<<", "<<(k+1)<<endl;
      break;}
  }
  }
  }
  if(c==0){
  cout<<"sequence not found"<<endl;}


  loop--;
  }

  return 0;
 }
